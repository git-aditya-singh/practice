//maxsum contigous sub array or kadane algo
public int maxSubArray(final List<Integer> a) {
        int maxsum=-(int)1e9;
        int s=0;
        for(int i=0;i<a.size();i++){
        s+=a.get(i);
        maxsum=Math.max(s,maxsum);
        if(s<0)
        {
            s=0;
        }
            
        }
        return maxsum;
    }
